{
  "metadata": {
    "spec-version": "1.5"
  },
  "display-name" : "BigQuery",
  "configuration-groups": [
    {
      "label": "Basic",
      "properties": [
        {
          "widget-type": "textbox",
          "label": "Reference Name",
          "name": "referenceName",
          "widget-attributes" : {
            "placeholder": "Name used to identify this sink for lineage"
          }
        },
        {
          "widget-type": "textbox",
          "label": "Dataset",
          "name": "dataset",
          "widget-attributes" : {
            "placeholder": "Dataset the table belongs to"
          }
        },
        {
          "widget-type": "textbox",
          "label": "Table",
          "name": "table",
          "widget-attributes" : {
            "placeholder": "Table to write to"
          }
        },
        {
          "widget-type": "textbox",
          "label": "Temporary Bucket Name",
          "name": "bucket",
          "widget-attributes" : {
            "placeholder": "Google Cloud Storage bucket for temporary data"
          }
        }
      ]
    },
    {
      "label" : "Advanced",
      "properties" : [
        {
          "widget-type": "textbox",
          "label": "Project Id",
          "name": "project",
          "widget-attributes" : {
            "default": "auto-detect"
          }
        },
        {
          "widget-type": "textbox",
          "label": "Service Account File Path",
          "name": "serviceFilePath",
          "widget-attributes" : {
            "default": "auto-detect"
          }
        }
      ]
    }
  ],
  "outputs": [
    {
      "name": "schema",
      "label": "schema",
      "widget-type": "schema",
      "widget-attributes": {
        "schema-types": [
          "string",
          "boolean",
          "int",
          "long",
          "float",
          "double",
          "timestamp",
          "bytes",
          "array"
        ],
        "schema-default-type": "string"
      }
    }
  ],
  "jump-config": {
    "datasets": [
      {
        "ref-property-name": "referenceName"
      }
    ]
  }
}
